{
  "name": "uswds-sandbox",
  "private": true,
  "author": "USWDS",
  "license": "CC0-1.0",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "check-for-uswds-dist": "./build.sh",
    "copy-uswds-assets": "npm run uswds-copy-fonts & npm run uswds-copy-img & npm run uswds-copy-js",
    "federalist": "npm run init && npm run build && npm run uswds-build-sass",
    "init": "npm run check-for-uswds-dist && npm run copy-uswds-assets",
    "serve": "npx @11ty/eleventy --serve",
    "serve-and-watch": "concurrently \"npm:serve\" \"npm:watch\"",
    "start": "npm run init && npm run serve-and-watch",
    "uswds-build-sass": "gulp buildSass",
    "uswds-build-app": "gulp compile",
    "uswds-copy-assets": "gulp copyAssets",
    "uswds-copy-fonts": "gulp copyFonts",
    "uswds-copy-img": "gulp copyImages",
    "uswds-copy-js": "gulp copyJS",
    "uswds-copy-theme": "gulp copyTheme",
    "uswds-update": "npm update uswds && ./build.sh",
    "watch": "gulp watchSass",
    "test": "snyk test"
  },
  "dependencies": {
    "@uswds/uswds": "^3.3.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@uswds/compile": "^1.0.0-beta.3",
    "concurrently": "^7.6.0",
    "snyk": "^1.1069.0"
  }
}
